<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wiraazharan.ayrell" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <uses-feature android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".VideoOne" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoTwo" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoThree" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoFour" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoFive" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoSix" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoSeven" android:screenOrientation="portrait"></activity>

        <activity android:name=".VideoEight" android:screenOrientation="portrait"></activity>

        <receiver android:name=".ForceOpen"></receiver>
    </application>

</manifest>
